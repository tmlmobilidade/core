{
	"name": "@tmlmobilidade/core",
	"version": "1.0.0",
	"author": "Jo√£o de Vasconcelos & Jusi Monteiro",
	"license": "AGPL-3.0-or-later",
	"homepage": "https://github.com/tmlmobilidade/services#readme",
	"bugs": {
		"url": "https://github.com/tmlmobilidade/services/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tmlmobilidade/services.git"
	},
	"keywords": [
		"public transit",
		"tml",
		"transportes metropolitanos de lisboa",
		"services"
	],
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "rimraf ./dist && tsup",
		"clean": "sh cleanup.sh",
		"dev": "dotenv-run -- tsx src/stops/index.ts",
		"docker:local": "docker compose -f docker/local/compose.local.yaml up -d",
		"docker:local-down": "docker compose -f docker/local/compose.local.yaml down",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "jest",
		"test:prod": "jest --silent --coverage",
		"test:watch": "jest --watch"
	},
	"exports": {
		"./interfaces": {
			"import": "./dist/interfaces/index.mjs",
			"require": "./dist/interfaces/index.js",
			"types": "./dist/interfaces/index.d.ts"
		},
		"./lib": {
			"import": "./dist/lib/index.mjs",
			"require": "./dist/lib/index.js",
			"types": "./dist/lib/index.d.ts"
		},
		"./providers": {
			"import": "./dist/providers/index.mjs",
			"require": "./dist/providers/index.js",
			"types": "./dist/providers/index.d.ts"
		},
		"./types": {
			"import": "./dist/types/index.mjs",
			"require": "./dist/types/index.js",
			"types": "./dist/types/index.d.ts"
		},
		"./utils": {
			"import": "./dist/utils/index.mjs",
			"require": "./dist/utils/index.js",
			"types": "./dist/utils/index.d.ts"
		}
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.744.0",
		"@aws-sdk/s3-request-presigner": "3.744.0",
		"@dotenv-run/cli": "1.3.6",
		"@types/geojson": "7946.0.16",
		"bcryptjs": "2.4.3",
		"luxon": "3.5.0",
		"mergekit": "3.0.6",
		"mongodb": "6.13.0",
		"nodemailer": "6.10.0",
		"pg": "8.13.1",
		"trino-client": "0.2.6",
		"tunnel-ssh": "5.2.0",
		"uuid": "11.0.5",
		"zod": "3.24.1"
	},
	"devDependencies": {
		"@carrismetropolitana/eslint": "20250128.1601.13",
		"@tmlmobilidade/tsconfig": "20241213.1919.54",
		"@types/bcrypt": "5.0.2",
		"@types/jest": "29.5.14",
		"@types/luxon": "3.4.2",
		"@types/node": "22.13.1",
		"@types/nodemailer": "6.4.17",
		"jest": "29.7.0",
		"mongodb-memory-server": "10.1.3",
		"rimraf": "6.0.1",
		"ts-jest": "29.2.5",
		"tsc-alias": "1.8.10",
		"tsup": "8.3.6",
		"tsx": "4.19.2",
		"turbo": "2.4.0",
		"typescript": "5.7.3"
	},
	"packageManager": "npm@11.1.0"
}
