{
	"name": "@tmlmobilidade/services",
	"version": "1.0.0",
	"author": "Jo√£o de Vasconcelos & Jusi Monteiro",
	"license": "AGPL-3.0-or-later",
	"homepage": "https://github.com/tmlmobilidade/services#readme",
	"bugs": {
		"url": "https://github.com/tmlmobilidade/services/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tmlmobilidade/services.git"
	},
	"keywords": [
		"public transit",
		"tml",
		"transportes metropolitanos de lisboa",
		"services"
	],
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "rimraf ./dist && tsc -p tsconfig.prod.json && tsc-alias",
		"dev": "dotenv-run -- tsx src/stops/index.ts",
		"docker:local": "docker compose -f docker/local/compose.local.yaml up -d",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"exports": {
		"./interfaces": {
			"import": "./dist/interfaces/index.js",
			"require": "./dist/interfaces/index.js",
			"types": "./dist/interfaces/index.d.ts"
		},
		"./providers": {
			"import": "./dist/providers/index.js",
			"require": "./dist/providers/index.js",
			"types": "./dist/providers/index.d.ts"
		},
		"./types": {
			"import": "./dist/types/index.js",
			"require": "./dist/types/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"dependencies": {
		"@dotenv-run/cli": "^1.3.6",
		"bcrypt": "^5.1.1",
		"mongodb": "^6.10.0",
		"pg": "^8.13.1",
		"trino-client": "^0.2.3",
		"tunnel-ssh": "^5.1.2",
		"uuid": "^11.0.2"
	},
	"devDependencies": {
		"@carrismetropolitana/eslint": "20241017.1618.32",
		"@tmlmobilidade/tsconfig": "^20241031.1431.3",
		"@types/bcrypt": "^5.0.2",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.8.5",
		"jest": "^29.7.0",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"tsc-alias": "^1.8.10",
		"turbo": "^2.2.3",
		"typescript": "^5.6.3"
	},
	"packageManager": "pnpm@9.12.3"
}
