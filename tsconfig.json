{
	"extends": "@tmlmobilidade/tsconfig/base.json",
	"compilerOptions": {
		"rootDir": ".",
		"outDir": "dist",
		"paths": {
			"@/*": ["./src/*"],
			"@test/*": ["./test/*"]
		},
		"strictNullChecks": true,
		"types": ["jest"]
	},
	"include": ["src/**/*.ts", "test/**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
